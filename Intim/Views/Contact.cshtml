@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Contact>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@using Terratype;
@{
    Layout = "Master.cshtml";
}
<div class="container">
   <h1>@Umbraco.Field("pageTitle")</h1>
</div>
   <section class="section">

   <div class="container">
        <div class="row">
            <div class="col-md-6">
                @Html.Terratype(Model.Content.Map)
           </div>
            <div class="col col-md-6">
                @Umbraco.Field("pageContent")
            </div>        
        </div>
   </div>
</section>